[1mdiff --git a/nuxt-app/components/BlogForm.vue b/nuxt-app/components/BlogForm.vue[m
[1mnew file mode 100644[m
[1mindex 0000000..0b5095e[m
[1m--- /dev/null[m
[1m+++ b/nuxt-app/components/BlogForm.vue[m
[36m@@ -0,0 +1,51 @@[m
[32m+[m[32m<template>[m
[32m+[m[32m  <form @submit.prevent="submit">[m
[32m+[m[32m    <input v-model="title" placeholder="Title" />[m
[32m+[m[32m    <textarea v-model="content" rows="6" placeholder="Content"></textarea>[m
[32m+[m[32m    <div style="display:flex;gap:.5rem;justify-content:flex-end">[m
[32m+[m[32m      <button type="submit">{{ editing ? 'Save' : 'Publish' }}</button>[m
[32m+[m[32m      <button v-if="editing" type="button" @click="cancel">Cancel</button>[m
[32m+[m[32m    </div>[m
[32m+[m[32m  </form>[m
[32m+[m[32m</template>[m
[32m+[m
[32m+[m[32m<script setup>[m
[32m+[m[32mimport { ref, watch } from 'vue'[m
[32m+[m[32mimport { useBlogStore } from '~/stores/blogs'[m
[32m+[m[32mconst store = useBlogStore()[m
[32m+[m[32mconst config = useRuntimeConfig()[m
[32m+[m
[32m+[m[32mconst title = ref('')[m
[32m+[m[32mconst content = ref('')[m
[32m+[m
[32m+[m[32mconst editing = computed(() => !!store.currentEdit)[m
[32m+[m
[32m+[m[32mwatch(() => store.currentEdit, (val) => {[m
[32m+[m[32m  if (val) {[m
[32m+[m[32m    title.value = val.title[m
[32m+[m[32m    content.value = val.content[m
[32m+[m[32m  } else {[m
[32m+[m[32m    title.value = ''[m
[32m+[m[32m    content.value = ''[m
[32m+[m[32m  }[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mconst submit = async () => {[m
[32m+[m[32m  const t = title.value.trim()[m
[32m+[m[32m  const c = content.value.trim()[m
[32m+[m[32m  if (!t || !c) return[m
[32m+[m[32m  if (editing.value) {[m
[32m+[m[32m    await store.updateBlog(config.public.apiBase, store.currentEdit.id, t, c)[m
[32m+[m[32m  } else {[m
[32m+[m[32m    await store.addBlog(config.public.apiBase, t, c)[m
[32m+[m[32m  }[m
[32m+[m[32m  title.value = ''[m
[32m+[m[32m  content.value = ''[m
[32m+[m[32m}[m
[32m+[m[32mconst cancel = () => store.cancelEditing()[m
[32m+[m[32m</script>[m
[32m+[m
[32m+[m[32m<style scoped>[m
[32m+[m[32minput, textarea { width:100%; padding:.6rem; border-radius:8px; border:1px solid #ddd; margin-bottom:.5rem }[m
[32m+[m[32mbutton { background:linear-gradient(90deg,#667eea,#764ba2); color:#fff; border:none; padding:.5rem .8rem; border-radius:8px }[m
[32m+[m[32m</style>[m
