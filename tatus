[36m@@ -0,0 +1,40 @@[m
[32m+[m[32m<template>[m
[32m+[m[32m  <div>[m
[32m+[m[32m    <div v-if="blogs.length">[m
[32m+[m[32m      <div v-for="b in blogs" :key="b.id" class="card">[m
[32m+[m[32m        <div v-if="store.currentEdit && store.currentEdit.id === b.id">[m
[32m+[m[32m          <!-- editing is handled in BlogForm; this is normal view -->[m
[32m+[m[32m          <small>Editing...</small>[m
[32m+[m[32m        </div>[m
[32m+[m[32m        <div v-else>[m
[32m+[m[32m          <h3>{{ b.title }}</h3>[m
[32m+[m[32m          <p>{{ b.content }}</p>[m
[32m+[m[32m          <small>{{ formatDate(b.created_at) }}</small>[m
[32m+[m[32m          <div style="text-align:right;margin-top:.5rem">[m
[32m+[m[32m            <button @click="store.startEditing(b)">Edit</button>[m
[32m+[m[32m            <button @click="remove(b.id)">Delete</button>[m
[32m+[m[32m          </div>[m
[32m+[m[32m        </div>[m
[32m+[m[32m      </div>[m
[32m+[m[32m    </div>[m
[32m+[m[32m    <p v-else>No blogs yet.</p>[m
[32m+[m[32m  </div>[m
[32m+[m[32m</template>[m
[32m+[m
[32m+[m[32m<script setup>[m
[32m+[m[32mimport { computed } from 'vue'[m
[32m+[m[32mimport { useBlogStore } from '~/stores/blogs'[m
[32m+[m[32mconst store = useBlogSt